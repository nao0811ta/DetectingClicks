{
  "manifest_version": 2,

  "name": "Detecting Click",
  "description": "This extension shows how to dectect click for the current page",
  "version": "1.0",
  
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.1.1.js", "detectclicks.js"]
      }
   ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
